{
  "manifest_version": 3,
  "name": "reFresher for Fiverr",
  "version": "1.0",
  "description": "Build an attractive extension by react",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs"
  ]
}
